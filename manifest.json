{
  "name": "PinLock",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was created with the awesome extensionizr.com",
  "homepage_url": "http://google.com",
  "icons": {
    "16": "icons/Lock-Lock-icon-16.png",
    "48": "icons/Lock-Lock-icon.png",
    "128": "icons/Lock-Lock-icon-128.png"
  },
  "browser_action": {
    "default_popup": "src/options/index.html"
  },
  "default_locale": "en",
  "options_page": "src/options/settings.html",
  "permissions": [
    "tabs",
    "storage"
  ]
}
